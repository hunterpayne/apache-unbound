    "dependencies":[
      {
        "groupId":"junit",
        "artifactId":"junit",
        "version":"${junitVersion}",
        "scope":"test"
      }
    ],
    "build":{
      "outputDirectory":"target/classes",
      "testResources":[
        {
          "targetPath":"src/test/resources",
          "includes":[
            "src/test/resources"
          ]
        }
      ],
      "pluginManagement":[
        {
          "groupId":"org.codehaus.plexus",
          "artifactId":"plexus-component-metadata",
          "version":"${plexusVersion}",
          "executions":[
            {
              "goals":[
                "generate-metadata",
                "generate-test-metadata"
              ]
            }
          ]
        },
        {
          "groupId":"org.apache.maven.plugins",
          "artifactId":"maven-compiler-plugin",
          "version":"2.0.2",
          "configuration":{
            "source":1.5,
            "target":1.5
          }
        },
        {
          "groupId":"org.apache.maven.plugins",
          "artifactId":"maven-release-plugin",
          "configuration":{
            "tagBase":"https://svn.apache.org/repos/asf/maven/components/tags"
          }
        },
        {
          "groupId":"org.codehaus.modello",
          "artifactId":"modello-maven-plugin",
          "version":"${modelloVersion}",
          "executions":[
            {
              "id":"site-docs",
              "phase":"pre-site",
              "goals":[
                "xdoc",
                "xsd"
              ]
            },
            {
              "id":"standard",
              "goals":[
                "java",
                "xpp3-reader",
                "xpp3-writer"
              ]
            }
          ],
          "configuration":{
            "useJava5":true
          }
        },
        {
          "groupId":"org.apache.felix",
          "artifactId":"maven-bundle-plugin",
          "version":"1.0.0"
        },
        {
          "groupId":"org.apache.maven.plugins",
          "artifactId":"maven-surefire-plugin",
          "version":"2.4.2"
        },
        {
          "groupId":"org.apache.maven.plugins",
          "artifactId":"maven-assembly-plugin",
          "version":"2.2-beta-2"
        },
        {
          "groupId":"org.apache.maven.plugins",
          "artifactId":"maven-resources-plugin",
          "version":"2.4-SNAPSHOT"
        }
      ]
    },
    "profiles":[
      {
        "id":"osgi",
        "build":{
          "testResources":[
            {
              "targetPath":"src/test/resources",
              "includes":[
                "src/test/resources"
              ]
            }
          ],
          "plugins":[
            {
              "groupId":"org.apache.felix",
              "artifactId":"maven-bundle-plugin",
              "executions":[
                {
                  "goals":[
                    "manifest"
                  ]
                }
              ]
            },
            {
              "groupId":"org.apache.maven.plugins",
              "artifactId":"maven-jar-plugin",
              "version":"2.1",
              "configuration":{
                "archive" : { "manifestFile":"${project.build.outputDirectory}/META-INF/MANIFEST.MF" }
              }
            }
          ]
        }
      },
      {
        "id":"release",
        "build":{
          "testResources":[
            {
              "targetPath":"src/test/resources",
              "includes":[
                "src/test/resources"
              ]
            }
          ],
          "plugins":[
            {
              "groupId":"org.apache.maven.plugins",
              "artifactId":"maven-assembly-plugin",
              "executions":[
                {
                  "id":"make-assembly",
                  "phase":"package",
                  "goals":[
                    "single"
                  ]
                }
              ],
              "inherited":false,
              "configuration":{
                "finalName":"maven-${project.version}-src",
                "tarLongFileMode":"gnu",
                "descriptors":[
                  "src/main/assembly/src.xml"
                ]
              }
            }
          ]
        }
      },
      {
        "id":"strict",
        "build":{
          "testResources":[
            {
              "targetPath":"src/test/resources",
              "includes":[
                "src/test/resources"
              ]
            }
          ],
          "plugins":[
            {
              "groupId":"org.apache.maven.plugins",
              "artifactId":"maven-enforcer-plugin",
              "version":"1.0-alpha-3",
              "executions":[
                {
                  "id":"enforce-jdk-15",
                  "goals":[
                    "enforce"
                  ],
                  "configuration": {
                    "rules" : { "requireJavaVersion" : { "version":1.5 } }
                  }
                }
              ]
            }
          ]
        }
      }
    ]
  }
