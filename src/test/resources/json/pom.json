{
  "project":{
    "groupId":"org.apache.maven",
    "artifactId":"unbound",
    "version":"1.0.0",
    "name":"Unbound",
    "description":"Maven Unbound is a file level translator of Apache Maven POM files to and\n    from JSON and HOCON POM formats.  It provides both a library to support\n    these translations and a command line interface to translate between\n    XML, JSON and HOCON formats of POMs.  The CLI can be called with\n    the mvnu script in this directory.  \n    \u2013\u2013generate\u2013hocon and \n    \u2013\u2013generate\u2013json command line flags are added to\n    translate a Maven project from XML to JSON and/or HOCON.  Then mvnu can\n    be called just like mvn can but the JSON or HOCON POM files will generate\n    pom.xml files on the fly which will in turn be built using Maven.",
    "url":"https://github.com/hunterpayne/maven-unbound",
    "inceptionYear":"2019",
    "licenses":[
      {
        "name":"Apache License Version 2.0",
        "url":"https://www.apache.org/licenses/LICENSE-2.0.txt",
        "distribution":"repo",
        "comments":""
      }
    ],
    "developers":[
      {
        "id":"hunter",
        "name":"Hunter Payne",
        "email":"no-spam",
        "roles":[
          "developer"
        ],
        "timezone":"America/Los_Angeles"
      }
    ],
    "scm":{
      "connection":"scm:git:https://github.com/hunterpayne/maven-unbound.git",
      "developerConnection":"scm:git:https://github.com/hunterpayne/maven-unbound.git",
      "url":"https://github.com/hunterpayne/maven-unbound"
    },
    "issueManagement":{
      "system":"GitHub",
      "url":"https://github.com/hunterpayne/maven-unbound/issues"
    },
    "properties":{
      "project.build.sourceEncoding":"UTF-8",
      "version.scala.release":"2.12.8",
      "version.scala.plugin.macro":"2.1.1",
      "release.stamp":"${maven.build.timestamp}",
      "maven.compiler.source":"${version.java}",
      "maven.compiler.target":"${version.java}",
      "version.scala.zinc":"1.3.0-M2",
      "version.scalatest":"3.0.6",
      "release.epoch":"1.0.0",
      "version.java":"1.8",
      "revision":"${release.epoch}",
      "maven.build.timestamp.format":"",
      "version.scala.simple.epoch":"2.12",
      "version.scala.epoch":"2.12"
    },
    "dependencies":[
      {
        "groupId":"org.scala-lang",
        "artifactId":"scala-library",
        "version":"${version.scala.release}"
      },
      {
        "groupId":"org.scala-lang.modules",
        "artifactId":"scala-xml_${version.scala.epoch}",
        "version":"1.2.0"
      },
      {
        "groupId":"com.iheart",
        "artifactId":"ficus_${version.scala.epoch}",
        "version":"1.4.6",
        "exclusions":[
          {
            "groupId":"org.scala-lang",
            "artifactId":"scala-library"
          }
        ]
      },
      {
        "groupId":"org.json4s",
        "artifactId":"json4s-native_${version.scala.epoch}",
        "version":"3.6.6"
      },
      {
        "groupId":"org.apache.maven",
        "artifactId":"maven-model",
        "version":"3.6.1"
      },
      {
        "groupId":"org.apache.maven.shared",
        "artifactId":"maven-shared-utils",
        "version":"3.2.1"
      },
      {
        "groupId":"org.scalatest",
        "artifactId":"scalatest_${version.scala.epoch}",
        "version":"${version.scalatest}",
        "scope":"test",
        "exclusions":[
          {
            "groupId":"org.scala-lang.modules",
            "artifactId":"scala-xml_${version.scala.epoch}"
          }
        ]
      }
    ],
    "build":{
      "sourceDirectory":"src/main/scala",
      "testSourceDirectory":"src/test/scala",
      "outputDirectory":"target/classes",
      "testResources":[
        {
          "targetPath":"src/test/resources",
          "includes":[
            "src/test/resources"
          ]
        }
      ],
      "plugins":[
        {
          "groupId":"org.apache.maven.plugins",
          "artifactId":"maven-compiler-plugin",
          "version":"3.7.0",
          "configuration":{
            "skip":true,
            "skipMain":true
          }
        },
        {
          "groupId":"net.alchim31.maven",
          "artifactId":"scala-maven-plugin",
          "version":"4.0.2",
          "executions":[
            {
              "goals":[
                "compile",
                "testCompile"
              ]
            }
          ],
          "configuration":{
            "scalaVersion":"${version.scala.release}"
          }
        },
        {
          "groupId":"org.apache.maven.plugins",
          "artifactId":"maven-shade-plugin",
          "version":"3.2.1",
          "executions":[
            {
              "phase":"package",
              "goals":[
                "shade"
              ],
              "configuration":{
                "shadedArtifactAttached":true,
                "shadedClassifierName":"exec",
                "minimizeJar":true,
                "transformers":[
                  {
                    "implementation":"org.apache.maven.plugins.shade.resource.ManifestResourceTransformer",
                    "mainClass":"org.apache.maven.unbound.Cli"
                  }
                ]
              }
            }
          ]
        },
        {
          "groupId":"org.scalatest",
          "artifactId":"scalatest-maven-plugin",
          "version":"1.0",
          "executions":[
            {
              "id":"test",
              "goals":[
                "test"
              ]
            }
          ],
          "configuration":{
            "parallel":false,
            "reportsDirectory":"${project.build.directory}/surefire-reports",
            "filereports":"TestSuite.txt",
            "junitxml":"."
          }
        },
        {
          "groupId":"org.apache.maven.plugins",
          "artifactId":"maven-dependency-plugin",
          "version":"3.1.1",
          "executions":[
            {
              "id":"copy",
              "phase":"package"
            }
          ]
        },
        {
          "groupId":"org.scalastyle",
          "artifactId":"scalastyle-maven-plugin",
          "version":"1.0.0",
          "executions":[
            {
              "goals":[
                "check"
              ]
            }
          ],
          "configuration":{
            "failOnWarning":false,
            "outputFile":"${project.build.directory}/site/scalastyle-output.xml",
            "configLocation":"${project.basedir}/scalastyle-config.xml",
            "includeTestSourceDirectory":false,
            "outputEncoding":"UTF-8",
            "sourceDirectory":"${project.basedir}/src/main/scala",
            "verbose":false,
            "testSourceDirectory":"${project.basedir}/src/test/scala",
            "failOnViolation":true
          }
        },
        {
          "groupId":"org.apache.maven.plugins",
          "artifactId":"maven-site-plugin",
          "version":"3.7.1"
        },
        {
          "groupId":"org.scoverage",
          "artifactId":"scoverage-maven-plugin",
          "version":"1.4.0-RC1",
          "executions":[
            {
              "id":"scoverage_unit",
              "phase":"site",
              "goals":[
                "report"
              ]
            }
          ],
          "configuration":{
            "scalaVersion":"${version.scala.release}",
            "minimumCoverage":"0",
            "highlighting":true,
            "failOnMinimumCoverage":true,
            "excludedPackages":"java..*;scala..*"
          }
        },
        {
          "groupId":"org.apache.maven.plugins",
          "artifactId":"maven-project-info-reports-plugin",
          "version":"3.0.0",
          "executions":[
            {
              "phase":"site",
              "goals":[
                "index"
              ]
            }
          ]
        },
        {
          "groupId":"com.redhat.victims.maven",
          "artifactId":"security-versions",
          "executions":[
            {
              "phase":"package",
              "goals":[
                "check"
              ]
            }
          ]
        }
      ]
    },
    "reporting":{
      "plugins":[
        {
          "groupId":"net.alchim31.maven",
          "artifactId":"scala-maven-plugin",
          "version":"4.0.2",
          "configuration":{
            "jvmArgs":[
              "-Xms64m",
              "-Xmx1024m"
            ]
          }
        },
        {
          "groupId":"org.apache.maven.plugins",
          "artifactId":"maven-project-info-reports-plugin",
          "version":"3.0.0",
          "reportSets":[
            {
              "reports":[
                "index",
                "summary",
                "dependencies",
                "dependency-info",
                "dependency-convergence",
                "team",
                "licenses",
                "plugins",
                "scm",
                "issue-management"
              ]
            }
          ]
        },
        {
          "groupId":"org.apache.maven.plugins",
          "artifactId":"maven-surefire-report-plugin",
          "version":"3.0.0-M3"
        },
        {
          "groupId":"org.apache.maven.plugins",
          "artifactId":"maven-dependency-plugin",
          "version":"3.1.1"
        },
        {
          "groupId":"org.codehaus.mojo",
          "artifactId":"findbugs-maven-plugin",
          "version":"3.0.5",
          "configuration":{
            "omitVisitors":"MutableStaticFields,FindNullDeref,MethodReturnCheck,UnreadFields"
          }
        },
        {
          "groupId":"org.scoverage",
          "artifactId":"scoverage-maven-plugin",
          "version":"1.4.0-RC1",
          "reportSets":[
            {
              "reports":[
                "report"
              ]
            }
          ]
        }
      ]
    }
  }
}
